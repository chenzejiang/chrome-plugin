{	
	"name": "btc-exchange",
	"manifest_version": 2,
	"version": "1.0.0",
	"description": "__MSG_pluginDesc__",
	"icons":
	{
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action": 
	{
		"default_icon": "img/icon.png",
		"default_title": "配置比特币主流交易所的访问地址",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/jquery-1.8.3.js", "js/content-script.js"],
			"css": [],
			"run_at": "document_start"
		}
	],
	"permissions":
	[
		"storage",
		"<all_urls>"
	],
	"options_ui":
	{
		"page": "options.html",
		"open_in_tab": true
	},
	"web_accessible_resources": ["*"],
	"homepage_url": "https://github.com/chenzejiang/chrome-plugin/tree/master/btc-exchange",
	"omnibox": { "keyword" : "btc" },
	"default_locale": "zh_CN"
}